{
    "name": "PowerShell",
    "dockerFile": "Dockerfile",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-vscode.powershell",
        "msazurermtools.azurerm-vscode-tools"
    ],
    "postCreateCommand": "pwsh -c './scripts/Dependencies.ps1'",
    "remoteUser": "vscode",
    "remoteEnv": {
        "ARM_TENANT_ID": "",
        "ARM_SUBSCRIPTION_ID": ""
    }
}